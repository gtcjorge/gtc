{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "indent": ["error", "tab"],
        "no-console":0,
        "arrow-body-style": ["error", "as-needed"],
        "no-tabs": 0,
        "no-alert": 0
    },
    "env": {
        "browser": true,
        "node": true,
        "jquery": true,
        "greasemonkey": true
    }
}